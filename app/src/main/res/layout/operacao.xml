<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/registar"
        android:layout_width="196dp"
        android:layout_height="104dp"
        android:layout_marginBottom="77dp"
        android:layout_marginTop="64dp"
        android:background="@color/colorPrimaryDark"
        android:text="Registar prestação de serviço"
        app:layout_constraintBottom_toTopOf="@+id/registos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/registos"
        android:layout_width="196dp"
        android:layout_height="87dp"
        android:layout_marginBottom="103dp"
        android:text="Ver registos"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/registar" />

</android.support.constraint.ConstraintLayout>